<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="286dp"
    android:layout_height="154dp"
    android:layout_centerInParent="true"
    android:background="#acacac"
    tools:context="pickture.sopt20th.com.pickutre.Dialog.OnlyForPhotographerDialog">

    <TextView
        android:id="@+id/aofpd_msg_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:layout_marginTop="22dp"
        android:text="작가로 등록한 회원만\n사용할 수 있는 기능입니다."
        android:textSize="16sp"
        android:textColor="#282828"/>

    <Button
        android:id="@+id/aofpd_ok_btn"
        android:layout_width="181dp"
        android:layout_height="37dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/aofpd_msg_txt"
        android:layout_marginTop="27dp"
        android:text="확    인"
        android:textSize="13sp"
        android:textColor="#ffffff"
        android:background="#282828"/>

</RelativeLayout>
